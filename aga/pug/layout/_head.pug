    script.
        function loadCSS(url) {
            try {
                document.createStyleSheet(url);
            } catch(e) {
                var link = document.createElement('link');
                link.rel = 'stylesheet';
                //link.type = 'text/css';
                link.href = url;
                document.getElementsByTagName('head')[0].appendChild(link);
            }
        }
        loadCSS('../fonts/icommon/style.css')
        loadCSS('../js/swiper/swiper.min.css')
        loadCSS('../js/jquery-modal-video/modal-video.min.css')
